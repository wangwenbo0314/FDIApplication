<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:topRightRadius="5.0dip" android:bottomRightRadius="5.0dip" />
            <stroke android:color="@color/translucent" />
            <solid android:color="@color/translucent" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:topRightRadius="5.0dip" android:bottomRightRadius="5.0dip" />
            <stroke android:width="1.0dip" android:color="@color/translucent" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>
</selector>